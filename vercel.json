{
  "builds": [
    {
      "src": "epg_server.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/diyp_epg.json(.*)",
      "dest": "/epg_server.py"
    }
  ],
  "env": {
    "PYTHONIOENCODING": "utf-8",
    "FLASK_ENV": "production"
  }
}
