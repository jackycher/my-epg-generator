{
  "builds": [
    {
      "src": "epg_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/diyp_epg.json(.*)",
      "dest": "epg_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "epg_server.py"
    }
  ],
  "env": {
    "PYTHONIOENCODING": "utf-8",
    "FLASK_ENV": "production"
  }
}
